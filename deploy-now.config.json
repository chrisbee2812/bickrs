{
  "name": "tanstack-vite-app",
  "type": "static",
  "static": {
    "publicDir": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "errorPage": "/404.html"
  },
  "build": {
    "command": "npm run build",
    "outputDir": "dist"
  }
}